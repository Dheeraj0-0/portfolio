{
  "buildCommand": "npm run build && rm -rf dist-static && mkdir -p dist-static && cp -r dist/public/* dist-static/",
  "outputDirectory": "dist-static",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}